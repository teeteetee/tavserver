            | job
        #contact.col-xs-4
          a(style='display: block; color: #333; font-family: this; font-size: 3vw;', href='/contacts') Contact
      #footer.row
        .col-xs-12(style='font-family: skinny; padding: 5px; text-align: center;')
          a(style='color: #333;', href='/')
            h4(style='font-family: this; font-weight: lighter;') Top and views.
      script.
        $('#showmap').click(function() {
        $('#map').slideToggle('slow');
        if ($("#map").is(':visible')) {
        $("html, body").animate({scrollTop: $("#map").offset().top});
        }
        });
