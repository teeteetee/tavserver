     :css
      @font-face {
      font-family:roboto-thin;
      src:url('/bootstrap/fonts/Roboto-Thin.ttf');
      }
      @font-face {
      font-family:attentica;
      src:url('/bootstrap/fonts/Attentica.ttf');
      }
      @font-face {
      font-family:br;
      src:url('/bootstrap/fonts/bebasneue_regular.ttf');
      }
      h1 {
      font-family:attentica;
      font-size:10vw;
      border-bottom:1px solid #333;
      display:inline;
      color:#a6a6a6;
      }
      h3{
      font-family:br;
      color:#a6a6a6;
      }
      h2{
      font-family:roboto-thin;
      font-size:4vw;
      }
      h4{
      font-family:br;
      color:gray;
      letter-spacing:1px;
      }
      h5{
      font-family:br;
      color:black;
      letter-spacing:1px;
      font-size:4vw;
      }
      h6{
      font-family:br;
      color:white;
      font-size:2vw;
      }
      .container{
      padding:0px;
      margin:0px;
      width:100%;
      }
      .row{
      padding:0px;
      margin:0px;
      width:100%;
      }
      .col-xs-4{
      padding:0px;
      margin:0px;
      }
      #header{
      text-align:center;
      }
      #choice{
      text-align:center;
      }
      .item:hover{
      background-color:#ffc773 !important;
      }
      .map{
      margin-top:1.34em;
      height:40vh;
      }
      .circle{
      border-radius:50%;
      border:2px solid white;
      height:2.5vw;
      width:2.5vw;
      margin:5px;
      background-color:#00b74a;
      display:block;
      text-align:center;
      line-height:2.5vw;
      }
